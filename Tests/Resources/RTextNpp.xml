<NotepadPlus>
    <Languages>
        <Language name="RTextNpp" ext="atm" commentStart="#" />
    </Languages>
    <LexerStyles>
        <LexerType name="RTextNpp" desc="RText language file." ext="meta  atm40" excluded="no">
            <WordsStyle name="DEFAULT" styleID="0" fgColor="FFFFFF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="FF8000" bgColor="3F3F3F" fontName="" fontStyle="3" fontSize="" />
            <WordsStyle name="NOTATION" styleID="2" fgColor="00FFFF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="REFERENCE" styleID="3" fgColor="8080FF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="FLOAT" styleID="4" fgColor="008040" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="">if else for while</WordsStyle>
            <WordsStyle name="INTEGER" styleID="5" fgColor="00FF00" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="">if else for while</WordsStyle>
            <WordsStyle name="QUOTED_STRING" styleID="6" fgColor="FF80FF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="BOOLEAN" styleID="7" fgColor="00FFFF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="LABEL" styleID="8" fgColor="FF9D6F" bgColor="3F3F3F" fontName="" fontStyle="1" fontSize="" />
            <WordsStyle name="COMMAND" styleID="9" fgColor="11FFFF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="IDENTIFIER" styleID="10" fgColor="FF0000" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="TEMPLATE" styleID="11" fgColor="FF8000" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="ERROR" styleID="12" fgColor="FF0000" bgColor="FFFFFF" fontName="" fontStyle="7" fontSize="" />
            <WordsStyle name="SPACE" styleID="13" fgColor="3F3F3F" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
            <WordsStyle name="OTHER" styleID="14" fgColor="FFFFFF" bgColor="3F3F3F" fontName="" fontStyle="0" fontSize="" />
        </LexerType>
    </LexerStyles>
</NotepadPlus>
